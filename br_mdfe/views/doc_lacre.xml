<odoo>

    <record model="ir.ui.view" id="doc_lacre_form_view">
        <field name="name">doc.lacre.form</field>
        <field name="model">doc.lacre</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="name"/>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="doc_lacre_tree_view">
        <field name="name">doc.lacre.tree</field>
        <field name="model">doc.lacre</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>
